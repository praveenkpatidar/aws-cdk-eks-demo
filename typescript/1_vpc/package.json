    "aws-cdk": "2.30.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.53.0",
    "console": "^0.7.2",
    "constructs": "^10.0.0",
    "fs": "^0.0.1-security",
    "load-yaml-file": "^1.0.0",
